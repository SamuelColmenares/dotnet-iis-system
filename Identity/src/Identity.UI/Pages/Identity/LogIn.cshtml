@page
@using UI.Dtos
@model UI.Pages.Identity.LogInModel
@{
}

<div>
    <h1>Log In</h1>
    <form method="post">
        <div>
            <label asp-for="LogIn.Email"></label>
            <input asp-for="LogIn.Email" type="email" />
            <span asp-validation-for="LogIn.Email"></span>
        </div>
        <div>
            <label asp-for="LogIn.Password"></label>
            <input asp-for="LogIn.Password" type="password" />
            <span asp-validation-for="LogIn.Password"></span>
        </div>
        <button type="submit">Log In</button>
    </form>
</div>